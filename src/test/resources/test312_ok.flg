  match `42L` with
  | `bv_big_const(42L)` => true
  | _ => false
  end.
  
ok3 :-
  match `42F` with
  | `fp_const(42F)` => true
  | _ => false
  end.
  
ok4 :-
  match `42D` with
  | `fp_big_const(42D)` => true

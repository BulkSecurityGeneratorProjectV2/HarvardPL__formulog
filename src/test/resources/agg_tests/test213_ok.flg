type abc = a | b | c

fun max(X: abc, Y: abc) : abc =
  match (X, Y) with
  | (c, _) | (_, c) => c
  | (b, _) | (_, b) => b
  | _ => a
  end
  
input bar(abc)
bar(a).
bar(b).
bar(c).

output foo(abc, abc<max,a>)
foo(X, X) :- bar(X).

output not_ok
not_ok :- foo(X, Y), X != Y.
not_ok :- foo(a, b).
not_ok :- foo(a, c).
not_ok :- foo(b, a).
not_ok :- foo(b, c).
not_ok :- foo(c, a).
not_ok :- foo(c, b).

output ok
ok :- foo(a, a), foo(b, b), foo(c, c), !not_ok. 
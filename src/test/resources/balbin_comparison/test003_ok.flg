input g(i32).
input b(i32, i32).
input k(i32).
input j(i32, i32).
output s(i32).
output i(i32).
output ok.

s(X) :- g(X).
s(X) :- b(X, Y), s(Y).
i(X) :- k(X).
i(X) :- j(X, Y), i(Y), !s(X). (* moved !s(X) to end to avoid "Rule cannot be evaluated given the supplied order" error *)
g(0).
g(1).
b(0, 1).
k(0).
j(0, 1).
j(1, 0). (* BE is deriving i(1) *)

:- i(_).

